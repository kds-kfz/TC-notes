<?xml version="1.0" encoding="GBK"?>
<Transaction>
    <EBHead>                                                        <!-- ESB 报文头 -->
        <RequesterID></RequesterID>                                 <!-- 请求放标识  清填写 ECM 分配的系统号 -->
        <ResponderId>ECM001</ResponderId>                           <!-- 接收方标识  默认值 -->
        <ESBServiceId></ESBServiceId>                               <!-- ESB 服务编码：暂空 -->
        <MessageType>1</MessageType>                                <!-- 消息类型 -->
        <ESBReqTimestamp>20080519140000</ESBReqTimestamp>           <!-- 请求处理时间戳 按实际填写 -->
        <ESBRspTimestamp></ESBRspTimestamp>                         <!-- 应答处理时间戳 暂空 -->
        <Version>1.0</Version>                                      <!-- 报文版本 默认值 -->
        <Reserved></Reserved>                                       <!-- 保留域：暂空 -->
    </EBHead>
    <ESBBody>                                                       <!-- ESM 报文体 -->
        <AppRequest>                                                <!-- 请求报文 -->
            <AppReqHead>                                            <!-- 请求报文头 -->
                <TradeCode>CREATE</TradeCode>                       <!-- 交易码 不能修改 -->
                <ReqSerialNo></ReqSerialNo>                         <!-- 请求方报文流水：暂空 -->
                <TradeDate>20080519</TradeDate>                     <!-- 交易日期 按实际填写 -->
                <TradeTime>140000</TradeTime>                       <!-- 交易时间 按实际填写 -->
                <TradeSource>
                    <EnterpriseId>BOCOM</EnterpriseId>              <!-- 企业标识 默认值 -->
                    <TradeChannel>9</TradeChannel>                  <!-- 交易渠道 默认值 -->
                    <SubBankNo></SubBankNo>                         <!-- 分行号 清填写省份行 或 直属行份行号，总行填 01315999999 -->
                    <OraganNo></OraganNo>                           <!-- 机构号 按实际填写或留空 -->
                    <TellerNo>3320001</TellerNo>                    <!-- 柜员号 按实际填写或默认值 -->
                    <TermNote>tty001</TermNote>                     <!-- 终端号 按实际填写或默认值 -->
                    <ForeTradeCode></ForeTradeCode>                 <!-- 前台交易代码 暂空 -->
                    <TradeNote></TradeNote>                         <!-- 交易备注 可选 -->
                </TradeSource>
                <AuthInfo>
                    <AuthLevel></AuthLevel>                         <!-- 授权级别：暂空 -->
                    <Supervisor1></Supervisor1>                     <!-- 主管1：暂空 -->
                    <Supervisor2></Supervisor2>                     <!-- 主管2：暂空 -->
                    <Supv1Passwd></Supv1Passwd>                     <!-- 主管1密码：暂空 -->
                    <Supv2Passwd></Supv2Passwd>                     <!-- 主管2密码：暂空 -->
                    <AuthReason></AuthReason>                       <!-- 授权原因：暂空 -->
                </AuthInfo>
                <Reserved></Reserved>                               <!-- 保留域：暂空 -->
            </AppReqHead>
            <AppReqBody>                                            <!-- 请求报文体 -->
                <CommonReq>                                         <!-- 公共请求要素 -->    
                    <RequestLevel>1</RequestLevel>                  <!-- 请求级别 默认值 -->
                    <TotalSize>204800</TotalSize>                   <!-- 文件总大小 按实际填写 -->
                    <EventID></EventID>                             <!-- 事件号 必填，32位以内，每笔交易不能不能重复 -->
                    <EventType>1</EventType>                        <!-- 事件类型 默认值 -->
                </CommonReq>
                <BatchIndex></BatchIndex>
                <DocumentList>                                      <!-- 存储文档列表 -->
                    <Document>                                      <!-- 存储文档 -->
                        <DocumentID></DocumentID>                   <!-- 文档 ID. CREATE 交易该节点留空 -->
                        <DocumentType>01</DocumentType>             <!-- 文档类型 默认值 -->
                        <Location></Location>                       <!-- 可选节点，不管业务类型，直接指定存储位置，可选值：local,remote,local-remote -->
                        <BizType>XXX</BizType>                      <!-- 系统号 不同系统使用 在 ECM 申请的不同系统号 -->
                        <CaseID></CaseID>                           <!-- 文档业务案例号 32位以内，不同系统不能送相同CaseID，建议使用 EventID -->
                        <AttrType>4</AttrType>                      <!-- 文档属性类型 1 BatchIndex 2 DocumentIndex 3 2个都用 4 都没有 -->
                        <DocumentIndex></DocumentIndex>             <!-- 存储文档索引 新系统都为空 -->
                        <ResourceObjectList>                        <!-- 文档资源列表 -->
                            <ResourceObjectContent>                 <!-- 文档资源，可一个或多个 -->
                                <ResourceObjectName>1.jpg</ResourceObjectName>                                       <!-- 资源名 -->
                                <ResourceObjectFullName>C:\img\upload\YYYYMMDDHHMMSS\1.jpg</ResourceObjectFullName>  <!-- 资源文件全路径 -->
                            </ResourceObjectContent>
                            <ResourceObjectContent>
                                <ResourceObjectName>2.jpg</ResourceObjectName>
                                <ResourceObjectFullName>C:\img\upload\YYYYMMDDHHMMSS\2.jpg</ResourceObjectFullName>
                            </ResourceObjectContent>
                        </ResourceObjectList>
                    </Document>
                </DocumentList>
            </AppReqBody>
        </AppRequest>
    </ESBBody>
</Transaction>
